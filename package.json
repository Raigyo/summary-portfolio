  "customDistStructure": {
    "config": {
      ".js": "src/assets/js",
      ".map": "src/assets/maps",
      ".css": "src/assets/css",
      "src/assets/img": [
        ".jpg",
        ".jpeg",
        ".png",
        ".webp",
        ".gif"
